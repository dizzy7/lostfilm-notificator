{
    "name": "dizzy/lostfilm",
    "license": "MIT",
    "type": "project",
    "description": "Notification new episodes on website http://www.lostfilm.ru",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [
            "app/AppKernel.php"
        ]
    },
    "require": {
        "php": ">=5.6",
        "ext-mongo": "*",
        "symfony/symfony": "2.8.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/assetic-bundle": "2.7.*",
        "doctrine/mongodb-odm": "1.0.*",
        "doctrine/mongodb-odm-bundle": "3.0.*",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "1.3.*",
        "braincrafted/bootstrap-bundle": "2.1.*",
        "twbs/bootstrap-sass": "3.3.*",
        "jquery/jquery":  "1.11.1",
        "leafo/scssphp": "0.6.*",
        "eightpoints/guzzle-bundle": "4.4.*",
        "doctrine/doctrine-fixtures-bundle": "2.3.*",
        "sunra/php-simple-html-dom-parser": "1.5.*",
        "shaygan/telegram-bot-api-bundle": "dev-master#6c3ae40de0e94674fc7cb64328b7ebe1f1d9a2e8"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "fabpot/php-cs-fixer": "1.9.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.11.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.11.1.js",
                    "type": "file"
                }
            }
        }
    ]
}
